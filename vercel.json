{
    "version": 2,
    "public": true,
    "rewrites": [
        {
            "source": "/api/:path*",
            "destination": "/backend/api/index.ts"
        },
        {
            "source": "/:path*",
            "destination": "/:path*"
        }
    ],
    "functions": {
        "backend/api/index.ts": {
            "maxDuration": 10
        }
    }
}